[Unit]
Description=Schedule a Message every 1 minute
RefuseManualStart=no  # Allow manual starts
RefuseManualStop=no   # Allow manual stops

[Timer]
Persistent=true  #Execute job if it missed a run due to machine being off
OnBootSec=50  #Run 120 seconds after boot for the first time
OnCalendar=*-*-* *:*/1:00  #Run every 5 minute thereafter
Unit=autorun.service #File describing job to execute
 
[Install]
WantedBy=timers.target
